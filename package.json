{
  "name": "tracenotes",
  "displayName": "TraceNotes",
  "description": "An extension for code tracing and note-taking.",
  "version": "0.0.11",
  "publisher": "RobertttBS",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/RobertttBS/vscode-tracenotes.git"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tracenotes.collectTrace",
        "title": "TraceNotes: Collect Trace"
      },
      {
        "command": "tracenotes.exportMarkdown",
        "title": "TraceNotes: Export to Markdown"
      },
      {
        "command": "tracenotes.importTrace",
        "title": "TraceNotes: Import from Markdown"
      },
      {
        "command": "tracenotes.clearAll",
        "title": "TraceNotes: Clear All Traces"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "tracenotes-sidebar",
          "title": "TraceNotes",
          "icon": "$(bookmark)"
        }
      ]
    },
    "views": {
      "tracenotes-sidebar": [
        {
          "type": "webview",
          "id": "tracenotes.storyboard",
          "name": "Storyboard"
        }
      ]
    },
    "keybindings": [
      {
        "command": "tracenotes.collectTrace",
        "key": "alt+c",
        "mac": "alt+c",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "build": "node esbuild.js",
    "watch": "node esbuild.js --watch",
    "lint": "eslint src --ext ts,tsx"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/vscode": "^1.85.0",
    "@vscode/vsce": "^3.7.1",
    "esbuild": "^0.27.3",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-syntax-highlighter": "^16.1.0"
  }
}
